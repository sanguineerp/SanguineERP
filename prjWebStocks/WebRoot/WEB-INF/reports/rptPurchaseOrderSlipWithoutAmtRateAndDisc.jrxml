	<subDataset name="dsPODtl" uuid="4d082a3f-8779-4451-980b-d49390b3eb98">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="WebRoot\WEB-INF\NEW_DATAADAPTER.xml"/>
		<queryString>
			<![CDATA[SELECT  1 slno,   po.strProdCode,p.strProdName as strProdName, p.strUOM, po.dblOrdQty, po.dblPrice, po.dbldiscount,
po.dblOrdQty*po.dblPrice-po.dbldiscount dblamt,  p.strPartNo,po.dblWeight,p.strCalAmtOn, po.strProdChar,p.strSpecification,
po.strProcessCode,pr.strProcessName,po.strRemarks,ph.strEOE,po.strUpdate 
FROM  tblpurchaseorderdtl po  
INNER JOIN tblpurchaseorderhd ph ON po.strPOCode = ph.strPOCode and ph.strClientCode='024.001' 
INNER JOIN tblproductmaster p ON po.strProdCode = p.strProdCode and p.strClientCode='024.001' 
left outer JOIN tblprocessmaster pr ON po.strProcessCode = pr.strProcessCode and pr.strClientCode='024.001'  
where po.strPOCode='01POAA000001' and po.strClientCode='024.001'  
 order by po.intindex]]>
		</queryString>
		<field name="slno" class="java.lang.Long"/>
		<field name="strProdCode" class="java.lang.String"/>
		<field name="strProdName" class="java.lang.String"/>
		<field name="strUOM" class="java.lang.String"/>
		<field name="dblOrdQty" class="java.math.BigDecimal"/>
		<field name="dblPrice" class="java.math.BigDecimal"/>
		<field name="dbldiscount" class="java.math.BigDecimal"/>
		<field name="dblamt" class="java.math.BigDecimal"/>
		<field name="strPartNo" class="java.lang.String"/>
		<field name="dblWeight" class="java.math.BigDecimal"/>
		<field name="strCalAmtOn" class="java.lang.String"/>
		<field name="strProdChar" class="java.lang.String"/>
		<field name="strSpecification" class="java.lang.String"/>
		<field name="strProcessCode" class="java.lang.String"/>
		<field name="strProcessName" class="java.lang.String"/>
		<field name="strRemarks" class="java.lang.String"/>
		<field name="strEOE" class="java.lang.String"/>
		<field name="strUpdate" class="java.lang.String"/>
		<variable name="dblSubTotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{dblamt}]]></variableExpression>
		</variable>
		<variable name="srNo" class="java.lang.Double" calculation="Count">
			<variableExpression><![CDATA[$V{srNo}+new Double(1.0)]]></variableExpression>
			<initialValueExpression><![CDATA[new Double(0.0)]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="dsTC" uuid="7bc6bbc4-01c2-416d-afdf-dcf828f3f12c">
		<queryString>
			<![CDATA[select tc.strTCName,tcDtl.strTCDesc 
from tbltcmaster tc inner join tbltctransdtl tcDtl on tc.strTCCode=tcDtl.strTCCode 
 and tc.strClientCode=tcDtl.strClientCode 
 where tcDtl.strTransCode='01POAL000002' and tcDtl.strTransType="Purchase Order" and tc.strClientCode="001" 
 group by tc.strTCCode]]>
		</queryString>
		<field name="strTCName" class="java.lang.String"/>
		<field name="strTCDesc" class="java.lang.String"/>
	</subDataset>
