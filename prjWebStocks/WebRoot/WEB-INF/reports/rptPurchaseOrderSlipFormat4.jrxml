((po.dblDiscount*100)/po.dblTotal) as dblDisPer 
FROM tblpurchaseorderhd AS po  left outer JOIN  tblpartymaster AS s ON po.strSuppCode = s.strPCode and s.strClientCode='024.001'
left outer join  tbluserhd u on po.strUserModified=u.strUserCode and u.strClientCode='024.001' 
left outer join tblcurrencymaster c on po.strCurrency=c.strShortName and c.strClientCode='024.001'
